      <!-- Page Wrapper -->
      <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark toggled">

          <!-- Home Logo -->
          <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/main">
